<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>KenoGrabbingServlet</servlet-name>
		<display-name>KenoGrabbingServlet</display-name>
		<description></description>
		<servlet-class>com.grabbing.task.KenoGrabbingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CheckResultServlet</servlet-name>
		<display-name>CheckResultServlet</display-name>
		<description></description>
		<servlet-class>com.grabbing.task.CheckResultServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KenoGrabbingServlet</servlet-name>
		<url-pattern>/KenoGrap</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CheckResultServlet</servlet-name>
		<url-pattern>/CheckResult</url-pattern>
	</servlet-mapping>
</web-app>
